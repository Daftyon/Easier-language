
PROGRAM Part10
{
    function fib(n: int) {
        if n < 1 {
            return 0;
        }
        elif n < 3 {
            return 1;
        }
        return fib(n - 1) + fib(n - 2);
    }
    function fact(n: int){
        if n<2 {
            return 1;
        }
        else{
            return n*fact(n-1);
        }
    }
   

    for i = 0; i < 10; i = i + 1 {
        print(fact(i));
    }

}